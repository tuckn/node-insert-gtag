{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./dist/", "./README.md"],
    "includePattern": ".js$",
    "excludePattern": "(./node_modules/|./test/|.test.js)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/docdash"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false,
      "useLongnameInNav": true
    }
  },
  "docdash": {
    "static": true,
    "sort": true,
    "disqus": "",
    "openGraph": {
      "title": "Node.js: insert-gtag",
      "type": "website",
      "image": "https://docs.tuckn.net/node-insert-gtag/img/openGraph-image.png",
      "site_name": "@tuckn/insert-gtag",
      "url": "https://github.com/tuckn/node-insert-gtag"
    },
    "meta": {
      "title": "node-insert-gtag",
      "description": "Node.js CLI to insert Google Tag Manager and Google Analytics code into your HTML files.",
      "keyword": "cli, html, gtag.js, tracking-code, analytics, google"
    },
    "search": true,
    "collapse": false,
    "wrap": false,
    "typedefs": true,
    "removeQuotes": "none",
    "menu": {
      "Github repo": {
        "href": "https://github.com/tuckn/node-insert-gtag",
        "target": "_blank",
        "class": "menu-item",
        "id": "repository"
      }
    }
  }
}
